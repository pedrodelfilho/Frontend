@model List<Interface.Models.BreadCrumpModel>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">&nbsp;</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    @{
                        foreach (var item in Model)
                        {
                            <li class="breadcrumb-item@(item.Ativo?" active":"")">
                                @if(item.Ancora)
                                {
                                <a href="#">@(item.Nome)</a>
                                }
                                else
                                {
                                @(item.Nome)
                                }
                            </li>
                        }
                    }
                </ol>
            </div>
        </div>
    </div>
</div>
